@*<div class="mask">
        <div class="aside left">

        </div>
    </div>*@

<script>
    $(function () {
        $("#lookuptest").leeLookup();
        $("#file").leeUpload();
        $("#checklist").leeRadioList({ data: [{ "id": "01", text: "蒙古" }, { "id": "02", text: "蒙古" }] });
        $("#datetest").leeDate();
        var options = {
            isMul: true,
            labelMode: true,
            limitCount: 12,
            url: "/FormBuilder.Web/Demo/Query",
            data: [{
                "id": "13", // value值
                "name": "Betty Deborah Jackson" // 名称
            },
            {
                "id": "14", // value值
                "name": "Betty Deborah Jackson" // 名称
            },
            {
                "id": "15", // value值
                "name": "Betty Deborah Jackson" // 名称
            }]
        };

        //$("#select").dropdown(options);


        //$('.dropdown-mul-1').dropdown({
        //    limitCount: 40,

        //    choice: function () {
        //        console.log(arguments, this);
        //    }
        //});

        $("#choosen").leeChoosen(options);

    });
</script>
@*输出上下文配置信息*@

<link href="/FormBuilder.Web/DList/webupload/webuploader.css" rel="stylesheet" />

@section ExternalScripts{

    @Scripts.Render("~/Bundle/Scripts/WebUploader")


}

<style>
    .lee-upload-files {
        border: 1px solid #DDD;
        padding: 10px;
        box-shadow: 0 1px 3px rgba(136, 136, 136, 0.2);
    }

    .webuploader-pick {
        background-color: #108ee9;
        border-color: #0e7fd1;
    }

    .webuploader-pick-hover {
        background-color: #31a0f1;
        border-color: #2c9ef1;
    }

    .progress .title {
        margin-bottom: 5px;
    }

    .progressbar {
        width: 300px;
        border: 1px solid #DDD;
        padding: 3px;
        margin-bottom: 5px;
    }

    .progressbar .line {
        height: 4px;
        transition: all ease .3s;
        background: rgba(33, 150, 243, 0.82);
        width: 30%;
    }

    .list .item {
        margin: 5px;
        padding: 7px 15px;
        display: inline-block;
        position: relative;
        padding-right: 25px;
        background: #eff5fe;
        background: rgba(170, 170, 170, 0.12);
        cursor: pointer;
        font-size: 13px;
        transition: all ease .3s;
    }

    .list .item a {
        text-decoration: underline;
        color: #F44336;
    }

    .list .item:hover {
    }

    .item span.op {
        margin-left: 3px;
        display: inline-block;
        position: absolute;
        top: 6px;
        right: 8px;
        transition: all ease .3s;
    }

    .list .item .op:hover {
        color: royalblue;
        transform: rotate(90deg);
    }



    .lee-radiolist-table .lee-radio-wrapper, .lee-radiolist-table label {
        font-size: 12px;
        margin-left: 2px;
    }





    .file-list {
        width: 100%;
        overflow: auto;
    }




    /*卡片展现模式*/

    .file-item {
        line-height: 1.428571429;
        background-color: #fff;
        border: 1px solid #ddd;
        border-radius: 4px;
        -webkit-transition: all .2s ease-in-out;
        transition: all .2s ease-in-out;
        width: 140px;
        height: 140px;
        float: left;
        position: relative;
        margin: 0 20px 20px 0;
        padding: 4px;
        -webkit-box-shadow: 0 1px 2px rgba(0,0,0,0.075);
        box-shadow: 0 1px 2px rgba(0,0,0,0.075);
    }

    .file-item img {
        width: 100%;
    }

    .file-item .info, .file-item .panel, .file-item .error {
        position: absolute;
        left: 4px;
        right: 4px;
        height: 20px;
        font-size: 14px;
        line-height: 20px;
        color: white;
    }

    .file-item .error {
        background: red;
        top: 4px;
        color: white;
        text-align: center;
    }



    .file-item .info {
        bottom: 4px;
        text-indent: 5px;
        background: rgba(0, 0, 0, 0.6);
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
        font-size: 12px;
        z-index: 10;
    }

    .file-item .panel {
        height: 24px;
        opacity: 0;
        top: 4px;
        line-height: 24px;
        transition: all ease .3s;
        background: rgba(0, 0, 0, 0.6);
    }

    .file-item .panel .op {
        float: right;
        margin-right: 8px;
        cursor: pointer;
    }

    .file-item:hover .panel {
        transform: translateY(0px);
        opacity: 1;
    }
</style>
